{
  "name": "ecolink",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:e2e",
    "lighthouse": "lighthouse http://localhost:5173 --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "setup:supabase": "node setup-supabase.js",
    "setup:accounts": "node setup-test-accounts.js",
    "test:supabase": "node test-supabase-integration.js",
    "test:integration": "node test-complete-integration.js",
    "test:integration:fixed": "node test-integration-fixed.js",
    "test:simple": "node simple-integration-test.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "happy-dom": "^15.10.2",
    "jsdom": "^23.0.0",
    "lighthouse": "^12.8.2",
    "msw": "^2.0.0",
    "prettier": "3.6.2",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^1.0.0"
  }
}
